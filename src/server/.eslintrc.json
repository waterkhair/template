{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:all",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "dot-location": "off",
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "max-len": "off",
        "max-params": [
            "error",
            5
        ],
        "newline-after-var": "off",
        "no-console": "off",
        "no-process-env": "off",
        "no-shadow": "off",
        "no-ternary": "off",
        "no-underscore-dangle": "off",
        "no-var": "off",
        "object-curly-newline": [
            "error", {
                "ObjectExpression": "always",
                "ObjectPattern": {
                    "multiline": true
                }
            }
        ],
        "one-var": "off",
        "padded-blocks": [
            "error",
            "never"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "quote-props": "off",
        "semi": [
            "error",
            "always"
        ],
        "space-before-function-paren": "off"
    }
}